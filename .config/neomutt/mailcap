# HTML and plain text files
text/*; $EDITOR %s;
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump %s; nametemplate=%s.html; copiousoutput;

# Other files will be opened with xdg-open
application/*; /home/nate/.config/neomutt/openfile %s;
audio/*; /home/nate/.config/neomutt/openfile %s;
image/*; /home/nate/.config/neomutt/openfile %s;
video/*; /home/nate/.config/neomutt/openfile %s;
